{
    "name": "fc-filepick",
    "version": "1.3.0",
    "description": "File Picker for Command Line Applications using enquirer",
    "main": "dist/index.js",
    "scripts": {
        "start": "nodemon run.js --watch src -I",
        "start:once": "node run.js",
        "build": "run-s build:bump build:src",
        "build:bump": "fsr bump",
        "build:all": "run-s build:src build:es6",
        "build:src": "parcel build src/index.js --no-cache --no-source-maps --target main",
        "build:es6": "parcel build src/index.js --no-cache --no-source-maps --target es6",
        "release": "run-s build release:npm",
        "release:npm": "npm publish",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "targets": {
        "main": {
            "context": "node",
            "isLibrary": true,
            "sourceMap": false,
            "distDir": "dist/commonjs",
            "outputFormat": "commonjs",
            "includeNodeModules": {
                "typescript": false,
                "fs": false,
                "path": false,
                "vscode": false
            }
        },
        "es6": {
            "context": "node",
            "isLibrary": true,
            "sourceMap": false,
            "distDir": "dist/es6",
            "outputFormat": "esmodule",
            "includeNodeModules": {
                "typescript": false,
                "vscode": false
            }
        }
    },
    "files": [
        "dist"
    ],
    "repository": "agrublev/fc-filepick",
    "license": "ISC",
    "keywords": [
        "browser",
        "cli",
        "file",
        "filebrowser",
        "picker"
    ],
    "author": "Angel Grablev <agrublev@gmail.com>",
    "dependencies": {
        "enquirer": "^2.4.1",
        "fc-chalk": "^1.0.0",
        "prompt": "^1.3.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.24.7",
        "@babel/core": "^7.24.7",
        "@babel/node": "^7.24.7",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-syntax-class-properties": "^7.12.13",
        "@babel/plugin-transform-runtime": "^7.24.7",
        "@babel/preset-env": "^7.24.7",
        "@babel/runtime": "^7.24.7",
        "fscr": "^5.4.1",
        "nodemon": "^3.1.4",
        "npm-run-all": "^4.1.5"
    }
}
