{
  "name": "fc-filepick",
  "version": "1.0.3",
  "description": "File Picker for Command Line Applications using enquirer",
  "main": "dist/index.js",
  "scripts": {
    "start": "nodemon run.js --watch src -I",
    "build": "run-s build:bump build:src",
    "build:bump": "fsr bump",
    "build:src": "parcel build src/index.js --no-cache --target node --bundle-node-modules --no-source-maps",
    "release": "run-s build release:npm",
    "release:npm": "npm publish",
    "test": "echo \"Error: no test specified\" && exit 1",
    "demo": "node demo.js",
    "docs:serve": "docsify serve docs"
  },
  "files": [
    "dist"
  ],
  "repository": "agrublev/fc-filepick",
  "license": "ISC",
  "keywords": [
    "browser",
    "cli",
    "file",
    "filebrowser",
    "picker"
  ],
  "author": "Angel Grablev <agrublev@gmail.com>",
  "dependencies": {
    "chalk": "^4.1.2",
    "enquirer": "^2.4.1",
    "prompt": "^1.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/node": "^7.23.9",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.4",
    "@babel/runtime": "^7.24.4",
    "fscr": "^5.4.1",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.12.0",
    "docsify-cli": "^4.4.4"
  }
}
