    "@babel/core": "^7.24.4",
    "@babel/node": "^7.23.9",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.4",
    "@babel/runtime": "^7.24.4",
    "babel-plugin-transform-import-extension": "^1.0.3",
    "docsify-cli": "^4.4.4",
