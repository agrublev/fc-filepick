{
  "name": "fc-filepick",
  "version": "1.0.0",
  "description": "File Picker for Command Line Applications",
  "main": "dist/index.js",
  "scripts": {
    "start": "nodemon run.js --watch src -I",
    "build": "parcel build src/index.js --no-cache --target node --bundle-node-modules --no-source-maps",
    "build:lib": "babel src --ignore 'node_modules' --out-dir dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Angel Grablev <agrublev@gmail.com>",
  "keywords": [
    "filebrowser",
    "file",
    "picker",
    "browser",
    "cli"
  ],
  "repository": "agrublev/fc-filepick",
  "license": "ISC",
  "dependencies": {
    "chalk": "^3.0.0",
    "enquirer": "^2.3.4",
    "prompt": "^1.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "@babel/plugin-syntax-class-properties": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@babel/node": "^7.6.3"
  }
}
